<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARTL_LeadNameIsMandatory</fullName>
    <active>false</active>
    <errorConditionFormula>OR(
   ISBLANK(FirstName),
   ISBLANK(TEXT(Salutation))
)</errorConditionFormula>
    <errorMessage>Please make sure Name(Salutation, FirstName, LastName) is required.</errorMessage>
</ValidationRule>
