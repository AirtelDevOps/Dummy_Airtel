<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARTL_LeadFNValidation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(  FirstName ,&quot;[a-zA-Z .]*&quot;))</errorConditionFormula>
    <errorDisplayField>FirstName</errorDisplayField>
    <errorMessage>First Name should not contain any Numbers/Special Characters.</errorMessage>
</ValidationRule>
