<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARTL_LeadLNValidation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( LastName ,&quot;[a-zA-Z .]*&quot;))</errorConditionFormula>
    <errorDisplayField>LastName</errorDisplayField>
    <errorMessage>Last Name should not contain any Numbers/Special Characters.</errorMessage>
</ValidationRule>
